{
  "name": "@cstleagueorg/cs2-blog-scraper",
  "version": "2.1.2",
  "description": "Scrap-scrap",
  "homepage": "https://github.com/CSTLeagueORG/CS2BlogScraper#readme",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint .",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "nodejs",
    "typescript",
    "jest",
    "cs2",
    "node-fetch"
  ],
  "author": "Sergey Zhirov <polarwolf@cstl.gg>",
  "repository": {
    "type": "git",
    "url": "https://github.com/CSTLeagueORG/CS2BlogScraper.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
